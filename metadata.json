{
  "name": "opuscodium-application",
  "version": "1.2.1-rc0",
  "source": "https://github.com/opus-codium/puppet-application",
  "author": "Opus Codium",
  "license": "Apache-2.0",
  "summary": "Tooling for Continuous Delivery (CD) of applications in a Puppet managed environment by leveraging orchestration through Bolt or Choria.",
  "project_page": "https://github.com/opus-codium/puppet-application",
  "issues_url": "https://github.com/opus-codium/puppet-application/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 6.5.0 < 9.0.0"
    },
    {
      "name": "puppetlabs-concat",
      "version_requirement": ">= 6.2.0 < 8.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "12",
        "13"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.16.0 < 8.0.0"
    }
  ]
}
